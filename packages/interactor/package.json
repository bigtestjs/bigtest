{
  "name": "@bigtest/interactor",
  "description": "Interaction library for testing big",
  "version": "0.9.3",
  "license": "MIT",
  "repository": "https://github.com/bigtestjs/interactor",
  "main": "dist/umd/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.js",
  "files": [
    "dist",
    "docs",
    "src"
  ],
  "scripts": {
    "types": "tsc --emitDeclarationOnly",
    "compile": "rollup --config",
    "build": "yarn types && yarn compile",
    "docs": "bigtest-docs",
    "lint": "eslint --ignore-path .gitignore ./",
    "start": "karma start",
    "test": "yarn start --single-run",
    "problems": "tsc --noEmit ",
    "prepack": "npm run docs && npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-transform-typescript": "^7.7.4",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@rollup/plugin-node-resolve": "^6.0.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.2",
    "@types/webpack-env": "^1.14.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-standard": "^4.0.0",
    "expect": "^25.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "karma": "4.4.1",
    "karma-chrome-launcher": "3.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-webpack": "4.0.2",
    "mocha": "~6.2.2",
    "regenerator-runtime": "0.13.3",
    "rollup": "^1.27.14",
    "rollup-plugin-babel": "^4.3.3",
    "typescript": "^3.7.4",
    "webpack": "^4.20.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@bigtest/convergence": "^1.1.1"
  }
}
